{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "GuestManager": {
      "root": "",
      "sourceRoot": "gm",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/gm",
            "index": "gm/index.html",
            "main": "gm/main.ts",
            "polyfills": "gm/polyfills.ts",
            "tsConfig": "gm/tsconfig.app.json",
            "assets": [
              "gm/favicon.ico",
              "gm/assets"
            ],
            "styles": [
              "gm/styles.scss",
              "node_modules/bootstrap/dist/css/bootstrap.css",
              "node_modules/font-awesome/css/font-awesome.css",
              "gm/assets/css/reset.scss",
              "gm/assets/css/strap-mins.scss",
              "gm/assets/css/grm.styles.scss"
            ],
            "scripts": [ "node_modules/jquery/dist/jquery.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.js"]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "gm/environments/environment.ts",
                  "with": "gm/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "GuestManager:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "GuestManager:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "GuestManager:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "gm/test.ts",
            "polyfills": "gm/polyfills.ts",
            "tsConfig": "gm/tsconfig.spec.json",
            "karmaConfig": "gm/karma.conf.js",
            "styles": [
            
              "gm/styles.scss"
            
            ],
            "scripts": [],
            "assets": [
              "gm/favicon.ico",
              "gm/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "gm/tsconfig.app.json",
              "gm/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "GuestManager-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "GuestManager:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "GuestManager:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "GuestManager"

}